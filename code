                                              text_color="#246EE9")
            SQ_title.place(x=250, y=40)
            SQ_close = customtkinter.CTkButton(SQ_window, text="Close Window", command=close, width=400, height=60)
            SQ_close.place(x=175, y=600)
            SQ_overview = customtkinter.CTkLabel(SQ_window,
                                                 text="This method offers a more efficient and active approach to reading\n textbook material.",
                                                 font=("Helvetica", 20, "normal"))
            SQ_overview.place(x=90, y=95)
            SQ_step1title = customtkinter.CTkLabel(SQ_window, text="Step 1 - Survey", font=("Helvetica", 30, "bold"))
            SQ_step1title.place(x=90, y=150)
            SQ_step1 = customtkinter.CTkLabel(SQ_window,
                                              text="Survey or skim the material to get an idea of what it is about",
                                              justify="left", font=("Helvetica", 20, "normal"))
            SQ_step1.place(x=90, y=190)
            SQ_step2title = customtkinter.CTkLabel(SQ_window, text="Step 2 - Question!", font=("Helvetica", 30, "bold"))
            SQ_step2title.place(x=90, y=250)
            SQ_step2 = customtkinter.CTkLabel(SQ_window,
                                              text="Create some questions that you have and that\nyou think the text might answer",
                                              justify="left", font=("Helvetica", 20, "normal"))
            SQ_step2.place(x=90, y=290)
            SQ_step3title = customtkinter.CTkLabel(SQ_window, text="Step 3 - Retrieve", font=("Helvetica", 30, "bold"))
            SQ_step3title.place(x=90, y=360)
            SQ_step3 = customtkinter.CTkLabel(SQ_window,
                                              text="Recall from memory the information you learned.\nUse your own words to formulate the material",
                                              justify="left", font=("Helvetica", 20, "normal"))
            SQ_step3.place(x=90, y=400)
            SQ_step4title = customtkinter.CTkLabel(SQ_window, text="Step 4 - Review!", font=("Helvetica", 30, "bold"))
            SQ_step4title.place(x=90, y=460)
            SQ_step4 = customtkinter.CTkLabel(SQ_window,
                                              text="Repeat back to yourself what the point of the material -\nwas, and summarize what you learned",
                                              justify="left", font=("Helvetica", 20, "normal"))
            SQ_step4.place(x=90, y=500)

        def space():
            global space_window
            menu.withdraw()
            space_window = CTkToplevel(menu)
            space_window.title("Spaced Repetition")
            space_window.geometry("750x700")
            space_window.maxsize(750, 700)
            space_title = customtkinter.CTkLabel(space_window, text="Spaced Repetition", font=("Helvetica", 40, "bold"),
                                                 text_color="#246EE9")
            space_title.place(x=200, y=40)
            space_close = customtkinter.CTkButton(space_window, text="Close Window", command=close, width=400,
                                                  height=60)
            space_close.place(x=175, y=600)
            space_overview = customtkinter.CTkLabel(space_window,
                                                    text="Spaced repetition is a learning strategy where information is repeatedly\nreviewed at increasing intervals to ensure that learnt info is unforgettable.",
                                                    font=("Helvetica", 20, "normal"))
            space_overview.place(x=50, y=95)
            space_step1title = customtkinter.CTkLabel(space_window, text="Step 1 - Write!",
                                                      font=("Helvetica", 30, "bold"))
            space_step1title.place(x=90, y=150)
            space_step1 = customtkinter.CTkLabel(space_window,
                                                 text="Turn information learnt into flash cards,\nincluding summaries, questions and answers",
                                                 justify="left", font=("Helvetica", 20, "normal"))
            space_step1.place(x=90, y=190)
            space_step2title = customtkinter.CTkLabel(space_window, text="Step 2 - Recall!",
                                                      font=("Helvetica", 30, "bold"))
            space_step2title.place(x=90, y=250)
            space_step2 = customtkinter.CTkLabel(space_window,
                                                 text="Go through these flashcards 2-3 days later,\nand progressively go into more depth",
                                                 justify="left", font=("Helvetica", 20, "normal"))
            space_step2.place(x=90, y=290)
            space_step3title = customtkinter.CTkLabel(space_window, text="Step 3 - Retrieve!",
                                                      font=("Helvetica", 30, "bold"))
            space_step3title.place(x=90, y=360)
            space_step3 = customtkinter.CTkLabel(space_window,
                                                 text="Repeat the process, but progressively recall the information in further",
                                                 justify="left", font=("Helvetica", 20, "normal"))
            space_step3.place(x=90, y=400)
            space_step4title = customtkinter.CTkLabel(space_window, text="Tip!", font=("Helvetica", 30, "bold"))
            space_step4title.place(x=90, y=460)
            space_step4 = customtkinter.CTkLabel(space_window,
                                                 text="Change the length of time intervals to your liking, in order\nto ensure you remember all the content written to a high degree.",
                                                 justify="left", font=("Helvetica", 20, "normal"))
            space_step4.place(x=90, y=500)

        def study_resources():
            global resources_frame
            try:
                if tab.winfo_exists():
                    tab.destroy()
            except NameError:
                pass

            try:
                if study_frame.winfo_exists():
                    study_frame.destroy()
            except NameError:
                pass

            try:
                if score_frame.winfo_exists():
                    score_frame.destroy()
            except NameError:
                pass
            try:
                if resources_frame.winfo_exists():
                    resources_frame.destroy()
            except NameError:
                pass

            # If frame already exists, it is destroyed to not conflict with home button
            resources_frame = customtkinter.CTkFrame(menu, width=1800, height=1150, corner_radius=1, fg_color="#F3EBF9")
            resources_frame.place(x=0, y=60)
            recommended_label = customtkinter.CTkLabel(resources_frame, text="Study Techniques", text_color="black",
                                                       font=("Helvetica", 50, "bold"))
            recommended_label.place(x=380, y=20)
            study_techniques = customtkinter.CTkLabel(resources_frame, text="Click any of the methods to learn more!",
                                                      text_color="red", font=("Helvetica", 20, "bold"))
            study_techniques.place(x=410, y=90)
            feynman_button = customtkinter.CTkButton(resources_frame, text="Feynman Technique", width=250, height=100,
                                                     fg_color="#286cec", font=("Helvetica", 20, "bold"), command=feyn)
            feynman_button.place(x=475, y=180)
            spaced_button = customtkinter.CTkButton(resources_frame, text="Spaced Repetition", width=250, height=100,
                                                    fg_color="#286cec", font=("Helvetica", 20, "bold"), command=space)
            spaced_button.place(x=825, y=180)
            SQ_button = customtkinter.CTkButton(resources_frame, text="SQ3R Method", width=250, height=100,
                                                fg_color="#286cec", font=("Helvetica", 20, "bold"), command=SQ)
            SQ_button.place(x=125, y=180)
            work_image = customtkinter.CTkImage(
                dark_image=Image.open("C:\\Users\\brodi\\PycharmProjects\\pythonProject\\work.png"),
                size=(300, 250))
            work_label = customtkinter.CTkLabel(resources_frame, image=work_image, text="")
            work_label.place(x=450, y=325)

        # Study score calculator function
        def study_score():
            global entry_ss1
            global entry_ss2
            global entry_ss3
            global entry_ss4
            global entry_ss5
            global entry_ss6
            global dropdown_ss1
            global dropdown_ss2
            global dropdown_ss3
            global dropdown_ss4
            global dropdown_ss5
            global dropdown_ss6
            global calculate_button
            global study_frame
            # If frame already exists, it is destroyed to not conflict with home button
            try:
                if study_frame.winfo_exists():
                    study_frame.destroy()
            except NameError:
                pass
            tab.destroy()
            # Subject Values
            subject_values = ["English", "English Language", "Literature", "Japanese", "German",
                              "Further Mathematics", "Mathematical Methods", "Specialist Mathematics",
                              "Business Management", "History", "Geography", "Economics", "Biology", "Chemistry",
                              "Physics",
                              "Psychology", "Software Development", "Drama",
                              "Agricultural and Horticultural Studies", "Visual Communication Design",
                              "Physical Education",
                              "Health and Human Development", "Legal Studies", "Food Studies",
                              "Product Design and Technology", "Media", "Systems Engineering"]
            sort_subject = sorted(subject_values)
            # Frame for Entry and Dropdown
            study_frame = customtkinter.CTkFrame(menu, width=1800, height=1150, corner_radius=1, fg_color="#F3EBF9")
            study_frame.place(x=0, y=60)
            global atar_title
            atar_title = customtkinter.CTkLabel(study_frame, text="Your Calculated ATAR:",
                                                font=("Comics Sans MS", 40, "bold"), text_color="black")
            atar_title.place(x=650, y=100)
            global subject_label
            subject_label = customtkinter.CTkLabel(study_frame, text="Subject Selection",
                                                   font=("Helvetica", 40, "bold"),
                                                   text_color="black")
            subject_label.place(x=60, y=30)
            global optional_label
            optional_label = customtkinter.CTkLabel(study_frame, text="Optional Subject",
                                                    font=("Helvetica", 40, "bold"), text_color="black")
            optional_label.place(x=60, y=460)
            # CTK entries for study score
            raw_ss = customtkinter.CTkLabel(study_frame, width=45, height=40, font=("Helvetica", 24, "bold"),
                                            text="Raw", text_color="black")
            raw_ss.place(x=395, y=65)
            ss_top = customtkinter.CTkLabel(study_frame, width=45, height=40, font=("Helvetica", 24, "bold"),
                                            text="Scaled", text_color="black")
            ss_top.place(x=460, y=65)
            entry_ss1 = customtkinter.CTkEntry(study_frame, width=45, height=40, font=("Helvetica", 24, "bold"),
                                               placeholder_text="SS")
            entry_ss1.place(x=400, y=100)
            entry_ss2 = customtkinter.CTkEntry(study_frame, width=45, height=40, font=("Helvetica", 24, "bold"),
                                               placeholder_text="SS")
            entry_ss2.place(x=400, y=175)
            entry_ss3 = customtkinter.CTkEntry(study_frame, width=45, height=40, font=("Helvetica", 24, "bold"),
                                               placeholder_text="SS")
            entry_ss3.place(x=400, y=250)
            entry_ss4 = customtkinter.CTkEntry(study_frame, width=45, height=40, font=("Helvetica", 24, "bold"),
                                               placeholder_text="SS")
            entry_ss4.place(x=400, y=325)
