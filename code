import customtkinter
from customtkinter import *
from PIL import Image
import random
from time import strftime

# Declare global variables for entry widgets
global firstname_entry
global lastname_entry
global username_entry
global password_entry


customtkinter.set_appearance_mode("dark")  # Modes: system (default), light, dark
customtkinter.set_default_color_theme("blue")  # Themes: blue (default), dark-blue, green
# Connecting to a database


def submit_sub():
    global firstname_entry
    global lastname_entry
    global username_entry
    global password_entry

    # Get the values from entry widgets
    fname = firstname_entry.get()
    lname = lastname_entry.get()
    username = username_entry.get()
    password = password_entry.get()

    # Get the selected values from the CTkOptionMenu widgets
    selected_subject1 = dropdown_subject1.get()
    selected_subject2 = dropdown_subject2.get()
    selected_subject3 = dropdown_subject3.get()
    selected_subject4 = dropdown_subject4.get()
    selected_subject5 = dropdown_subject5.get()
    selected_subject6 = dropdown_subject6.get()






# Function for signing out via tab
def sign_out():
    app.iconify()
    menu.withdraw()
    app.geometry("500x440+1000+300")
    app.deiconify()


# Function for returning back to main menu - destroys all possible windows
def home_page_command():
    # Try, expect statements to destroy any of the windows if they exist, otherwise pass.
    try:
        if tab.winfo_exists():
            tab.destroy()
    except NameError:
        pass

    try:
        if study_frame.winfo_exists():
            study_frame.destroy()
    except NameError:
        pass

    try:
        if score_frame.winfo_exists():
            score_frame.destroy()
    except NameError:
        pass
    try:
        if resources_frame.winfo_exists():
            resources_frame.destroy()
    except NameError:
        pass
    try:
        if disclaimer_frame.winfo_exists():
            disclaimer_frame.destroy()
    except NameError:
        pass


# Sign Up page function
def submit_details():
    global dropdown_subject1
    global dropdown_subject2
    global dropdown_subject3
    global dropdown_subject4
    global dropdown_subject5
    global dropdown_subject6

    vce_response = vce_option.get()
    if vce_response == "No":
        signup.destroy()
        app.deiconify()
    else:
        signup.destroy()
        subject_window = CTkToplevel(app)
        subject_window.geometry("1000x800")
        subject_window.title("Subject Selection")

        sub_1 = dropdown_subject1.get()
        sub_2 = dropdown_subject2.get()
        sub_3 = dropdown_subject3.get()
        sub_4 = dropdown_subject4.get()
        sub_5 = dropdown_subject5.get()
        sub_6 = dropdown_subject6.get()

        dropdown_subject1 = customtkinter.CTkOptionMenu(subject_window, values=["English Language", "English", "Literature"], width=400, height=40)
        dropdown_subject1.place(x=200, y=75)
        subject_values = ["English", "English Language", "Literature", "Japanese", "German",
                          "Further Mathematics", "Mathematical Methods", "Specialist Mathematics",
                          "Business Management", "Ancient History", "Geography", "Economics", "Biology", "Chemistry",
                          "Physics",
                          "Psychology", "Software Development", "Drama",
                          "Agricultural and Horticultural Studies", "Visual Communication Design", "Physical Education",
                          "Health and Human Development", "Legal Studies", "Food Studies",
                          "Product Design and Technology", "Media", "Systems Engineering"]
        sort_subject = sorted(subject_values)
        dropdown_subject2 = customtkinter.CTkOptionMenu(subject_window, values=sort_subject, width=400, height=40)
        dropdown_subject2.place(x=200, y=175)
        dropdown_subject3 = customtkinter.CTkOptionMenu(subject_window, values=sort_subject, width=400, height=40)
        dropdown_subject3.place(x=200, y=275)
        dropdown_subject4 = customtkinter.CTkOptionMenu(subject_window, values=sort_subject, width=400, height=40)
        dropdown_subject4.place(x=200, y=375)
        dropdown_subject5 = customtkinter.CTkOptionMenu(subject_window, values=sort_subject, width=400, height=40)
        dropdown_subject5.place(x=200, y=475)
        dropdown_subject6 = customtkinter.CTkOptionMenu(subject_window, values=sort_subject, width=400, height=40)
        dropdown_subject6.place(x=200, y=575)
        submit_subjects = customtkinter.CTkButton(subject_window, text="Submit Subjects", width=400, height=60,
                                                  fg_color="#619DB9", font=("Helvetica", 16, "bold"), command=submit_sub)
        submit_subjects.place(x=200, y=675)
        label_subject1 = customtkinter.CTkLabel(subject_window, text="Subject 1")
        label_subject1.place(x=200, y=25)
        label_subject2 = customtkinter.CTkLabel(subject_window, text="Subject 2")
        label_subject2.place(x=200, y=125)
        label_subject3 = customtkinter.CTkLabel(subject_window, text="Subject 3")
        label_subject3.place(x=200, y=225)
        label_subject4 = customtkinter.CTkLabel(subject_window, text="Subject 4")
        label_subject4.place(x=200, y=325)
        label_subject5 = customtkinter.CTkLabel(subject_window, text="Subject 5")
        label_subject5.place(x=200, y=425)
        label_subject6 = customtkinter.CTkLabel(subject_window, text="Subject 6 (Optional)")
        label_subject6.place(x=200, y=525)


def signup_window():
    global firstname_entr
    global lastname_entry
    global username_entry
    global password_entry
    global vce_option
    global signup
    global dropdown_subject1
    global dropdown_subject2
    global dropdown_subject3
    global dropdown_subject4
    global dropdown_subject5
    global dropdown_subject6

    def submit_signup_details():
        # Get the selected values from the CTkOptionMenu widgets
        selected_vce_option = vce_option.get()
        selected_subject1 = dropdown_subject1.get()
        selected_subject2 = dropdown_subject2.get()
        selected_subject3 = dropdown_subject3.get()
        selected_subject4 = dropdown_subject4.get()
        selected_subject5 = dropdown_subject5.get()
        selected_subject6 = dropdown_subject6.get()

        # Insert the selected subjects into the database
        subjects_query = "INSERT INTO subjects (username, sub_1, sub_2, sub_3, sub_4, sub_5, sub_6) VALUES (%s, %s, %s, %s, %s, %s, %s)"
        subjects_values = (
        username_entry.get(), selected_subject1, selected_subject2, selected_subject3, selected_subject4,
        selected_subject5, selected_subject6)

        try:
            cur.execute(subjects_query, subjects_values)
            conn.commit()
            print("Subjects saved successfully!")
        except Exception as e:
            print("Error:", e)
            conn.rollback()

    app.iconify()
    signup = CTkToplevel(app)
    signup.title("Registration")
    signup.geometry("750x600")
    firstname_entry = customtkinter.CTkEntry(signup, placeholder_text="First Name", width=300, height=40)
    firstname_entry.place(x=50, y=50)
    lastname_entry = customtkinter.CTkEntry(signup, placeholder_text="Last Name", width=300, height=40)
    lastname_entry.place(x=50, y=130)
    username_entry = customtkinter.CTkEntry(signup, placeholder_text="Username", width=300, height=40)
    username_entry.place(x=50, y=210)
    password_entry = customtkinter.CTkEntry(signup, placeholder_text="Password", width=300, height=40)
    password_entry.place(x=50, y=290)
    vce_label = customtkinter.CTkLabel(signup, text="Are you currently enrolled in VCE studies?", text_color="gray",
                                       font=("Helvetica", 16, "bold"))
    vce_label.place(x=50, y=365)
    vce_option = customtkinter.CTkOptionMenu(signup, values=["Yes", "No"], width=200, height=40)
    vce_option.place(x=50, y=400)

    dropdown_subject1 = customtkinter.CTkOptionMenu(signup, values=[], width=400, height=40)
    dropdown_subject2 = customtkinter.CTkOptionMenu(signup, values=[], width=400, height=40)
    dropdown_subject3 = customtkinter.CTkOptionMenu(signup, values=[], width=400, height=40)
    dropdown_subject4 = customtkinter.CTkOptionMenu(signup, values=[], width=400, height=40)
    dropdown_subject5 = customtkinter.CTkOptionMenu(signup, values=[], width=400, height=40)
    dropdown_subject6 = customtkinter.CTkOptionMenu(signup, values=[], width=400, height=40)

    submit_signup = customtkinter.CTkButton(signup, text="Submit Details", width=275, height=45, command=submit_sub)
    submit_signup.place(x=50, y=475)

    next_button = customtkinter.CTkButton(signup, text="Next", width=275, height=45, command=submit_details)
    next_button.place(x=50, y=535)

# Creating login application
app = customtkinter.CTk()  # create CTk window like you do with the Tk window
app.geometry("500x440+1000+300")
app.title("VLT Login Page")
app.maxsize(500, 440)
app.minsize(500, 440)

# Logo for login page
logo_login = customtkinter.CTkImage(
    dark_image=Image.open("C:\\Users\\brodi\\PycharmProjects\\pythonProject\\vltnew.png"),
    size=(150, 150))
# Binding image to a label, so it can be packed
output_login = customtkinter.CTkLabel(app, image=logo_login, text="")
output_login.place(x=167, y=5)

# Entry text box for username & password
entry_username = customtkinter.CTkEntry(app, placeholder_text="Username", width=400, height=40)
entry_username.place(x=53.5, y=125)
entry_password = customtkinter.CTkEntry(app, placeholder_text="Password", width=400, height=40, show="*")
entry_password.place(x=53.5, y=200)

# Sign up prompt
sign_up = customtkinter.CTkButton(app, command=signup_window, text="Sign Up", fg_color="transparent", text_color="red",
                                  hover_color="gray", font=("Helvetica", 16, "bold"))
sign_up.place(x=190, y=375)

def disclaimer():
    global disclaimer_frame
    try:
        if tab.winfo_exists():
            tab.destroy()
    except NameError:
        pass

    try:
        if study_frame.winfo_exists():
            study_frame.destroy()
    except NameError:
        pass

    try:
        if score_frame.winfo_exists():
            score_frame.destroy()
    except NameError:
        pass
    try:
        if disclaimer_frame.winfo_exists():
            disclaimer_frame.destroy()
    except NameError:
        pass
    try:
        if resources_frame.winfo_exists():
            resources_frame.destroy()
    except NameError:
        pass
    disclaimer_frame = customtkinter.CTkFrame(menu, width=1800, height=1150, corner_radius=1, fg_color="#F3EBF9")
    disclaimer_frame.place(x=0, y=60)
    disclaimer_label = customtkinter.CTkLabel(disclaimer_frame, text="Disclaimer:", text_color="red",
                                              font=("Helvetica", 40, "bold"))
    disclaimer_label.place(x=25, y=20)
    point_1 = CTkLabel(disclaimer_frame,
                       text="The calculations done on this application are intended for informational purposes",
                       text_color="black", font=("Helvetica", 18, "bold"))
    point_1.place(x=20, y=125)
    point_2 = CTkLabel(disclaimer_frame,
                       text="Calculations are based on previous statistics from VCAA and do not guarantee the ATAR or study score you may receieve.",
                       text_color="black", font=("Helvetica", 18, "bold"))
    point_2.place(x=20, y=200)
    point_3 = CTkLabel(disclaimer_frame,
                       text="We do not accept any liability for the decisions made from the calculations provided, this application should only be used as a simple reference.",
                       text_color="black", font=("Helvetica", 16, "bold"))
    point_3.place(x=20, y=275)
    point_4 = CTkLabel(disclaimer_frame,
                       text="By using this tool you acknowledge and accept the uncertainties associated with the services provided.",
                       text_color="black", font=("Helvetica", 18, "bold"))
    point_4.place(x=20, y=350)
    point_5 = CTkLabel(disclaimer_frame, text="VLT is not officially affiliated with the VCAA government agency",
                       text_color="black", font=("Helvetica", 18, "bold"))
    point_5.place(x=20, y=425)
    point_6 = CTkLabel(disclaimer_frame,
                       text="If you have any concerns or feedback contact us via: vlt@gmail.com",
                       text_color="black", font=("Helvetica", 18, "bold"))
    point_6.place(x=20, y=500)


# Study Score Calculator Function
def ss_calculator():
    global score_frame
    # If frame already exists, it is destroyed to not conflict with home button
    try:
        if score_frame.winfo_exists():
            score_frame.destroy()
    except NameError:
        pass
    # Creating study score page frame
    score_frame = customtkinter.CTkFrame(menu, width=1800, height=1150, corner_radius=1, fg_color="#F3EBF9")
    score_frame.place(x=0, y=60)
    tab.destroy()
    # Creating labels
    title_label = customtkinter.CTkLabel(score_frame, text="Study Hours for a subject - PER WEEK",
                                         font=("Helvetica", 40, "bold"), text_color="#75a9f9")
    title_label.place(x=250, y=50)
    calc_label = customtkinter.CTkLabel(score_frame, text="Calculated Study Score:", font=("Helvetica", 40, "bold"),
                                        text_color="#75a9f9")
    calc_label.place(x=380, y=325)
    hours_label = customtkinter.CTkLabel(score_frame, text="", font=("Helvetica", 40, "bold"), text_color="blue")
    hours_label.place(x=605, y=250)
    study_question = customtkinter.CTkLabel(score_frame, font=("Helvetica", 20, "bold"),
                                            text="How many hours have you studied?", text_color="black")
    study_question.place(x=450, y=150)
    # Importing images to display via a label
    bookss_image = customtkinter.CTkImage(
        dark_image=Image.open("C:\\Users\\brodi\\PycharmProjects\\pythonProject\\bookss.png"), size=(200, 200))
    bookss_imagelabel = customtkinter.CTkLabel(score_frame, image=bookss_image, text="")
    bookss_imagelabel.place(x=900, y=200)
    pen_image = customtkinter.CTkImage(
        dark_image=Image.open("C:\\Users\\brodi\\PycharmProjects\\pythonProject\\pen.png"), size=(250, 250))
    pen_imagelabel = customtkinter.CTkLabel(score_frame, image=pen_image, text="")
    pen_imagelabel.place(x=100, y=200)
    # Defining variables to be used later
    value = ""
    global hours_ss
    hours_ss = ""

    # Creating a slider function
    def slider_event(value):
        # Rounding the value of the slider
        global hours_ss
        hours_label.configure(text=round(value))
        # Converting study hours to study score
        if round(value) == 1:
            hours_ss = "10-15"
        elif round(value) == 2:
            hours_ss = "20-25"
        elif round(value) == 3:
            hours_ss = "26-30"
        elif round(value) == 4:
            hours_ss = "31-35"
        elif round(value) == 5:
            hours_ss = "36-40"
        elif round(value) == 6:
            hours_ss = "≥40"
        study_score_label.configure(text=hours_ss)

    study_score_label = customtkinter.CTkLabel(score_frame, text="", text_color="green", font=("Helvetica", 40, "bold"))
    study_score_label.place(x=565, y=425)
    # Creating slider
    slider = customtkinter.CTkSlider(score_frame, from_=1, to=6, number_of_steps=5, progress_color="green",
                                     command=slider_event, width=400)
    slider.place(x=410, y=215)


# Function to open the main menu window upon successful login
def open_main_menu():
    global menu
    app.withdraw()  # Hide the login window


# Function for opening the main menu window upon successful login
def open_main_menu():
    pass

    # Creating a label that dynamically displays time, including time, day, month, year
    def time():
        time_string = strftime('%d %b %H:%M %p')
        time_label.configure(text=time_string)
        time_label.after(1000, time)

    # Displays the time
    time_label = CTkLabel(menu, font=('calibri', 60, 'bold'), fg_color="#F3EBF9", text_color="black")
    time_label.place(x=650, y=350)
    time()



# Function for handling the login button click event
def button_login_event():
    # Get content of both entry widgets
    entered_username = entry_username.get()
    entered_password = entry_password.get()

    if entered_username == "admin" and entered_password == "password":
        print("Login successful!")
    else:
        incorrect_label = customtkinter.CTkLabel(app, text="Incorrect username or password", text_color="red")
        incorrect_label.place(x=157.5, y=248)
        return
    global menu
    global greetings_text
    app.withdraw()
    menu = CTkToplevel(app)
    menu.geometry("1200x650+500+300")
    menu.title("Main Menu")
    menu.maxsize(1200, 650)
    menu.minsize(1200, 650)
    menu.configure(fg_color="#F3EBF9")
    # Greetings label on menu
    greetings_text = customtkinter.CTkLabel(menu, text="Good morning, Brodie", width=200, height=100,
                                                fg_color="#F3EBF9", corner_radius=1, pady=40,
                                                font=("Tekton pro", 70, "bold"), text_color="#1c2424")
    greetings_text.place(x=50, y=100)
     # Importing main menu image
    ss_image = customtkinter.CTkImage(
        dark_image=Image.open("C:\\Users\\brodi\\PycharmProjects\\pythonProject\\calculator.png"), size=(400, 300))
    ss_imagelabel = customtkinter.CTkLabel(menu, image=ss_image, text="")
    ss_imagelabel.place(x=100, y=320)
     # List for quotes to be rotated
    quotes = ['"Most people who won big, failed big" - Tom Platz', '"Just do your best" - Anonymous',
                  '"If you can dream it, you can do it." - Walt Disney',
                  '"You define your own life. Dont let other people write your script" - Oprah',
                  '"If you know you can do better, do better" - Maya Angelou',
                  '"If your going through hell, keep going" - Winston Churchill',
                  '"Education is a weapon" - Joseph Stalin', ]
        # Chooses a random quote to be displayed in quote_label
    quote_random = random.choice(quotes)
    quote_label = customtkinter.CTkLabel(menu, text=quote_random, font=("Helvetica", 30, "bold"),
                                             text_color="#498fe0", height=50, width=10)
    quote_label.place(x=60, y=250)



    # Creating a label that dynamically displays time, including time, day, month, year
    def time():
        time_string = strftime('%d %b %H:%M %p')
        time_label.configure(text=time_string)
        time_label.after(1000, time)

    # Displays the time
    time_label = CTkLabel(menu, font=('calibri', 60, 'bold'), fg_color="#F3EBF9", text_color="black")
    time_label.place(x=650, y=350)
    time()


    # Function for opening the sidebar.
    def open():
        global tab
        # If frame already exists, it is destroyed to not conflict with home button
        try:
            if tab.winfo_exists():
                tab.destroy()
        except NameError:
            pass
        tab = customtkinter.CTkFrame(menu, width=300, height=1000, fg_color="gray", border_width=1, corner_radius=1)
        tab.place(x=0, y=-1)

        # Function for closing sidebar
        def delete():
            tab.destroy()

        # Importing exit icon image
        exit_image = customtkinter.CTkImage(
            dark_image=Image.open("C:\\Users\\brodi\\PycharmProjects\\pythonProject\\exit.png"),
            size=(50, 50))
        exit_button = customtkinter.CTkButton(tab, image=exit_image, text="", fg_color='gray', hover_color='gray',
                                              width=10, command=delete)
        exit_button.place(x=0, y=1)

        # Scaling Study Scores
        def convert():
            global atar_score
            global scaled_ss1

            ss1 = entry_ss1.get()
            ss1_int = int(ss1)
            if dropdown_ss1.get() == "English":
                if 1 <= ss1_int <= 10:
                    scaled_ss1 = ss1_int - 1
                elif 10 <= ss1_int <= 16:
                    scaled_ss1 = ss1_int - 2
                elif 17 <= ss1_int <= 27:
                    scaled_ss1 = ss1_int
                elif 28 <= ss1_int <= 37:
                    scaled_ss1 = ss1_int - 2
                elif 38 <= ss1_int <= 42:
                    scaled_ss1 = ss1_int - 1
                elif 43 <= ss1_int <= 50:
                    scaled_ss1 = ss1_int
            elif dropdown_ss1.get() == "Literature":
                if 1 <= ss1_int <= 22:
                    scaled_ss1 = ss1_int
                elif 23 <= ss1_int <= 32:
                    scaled_ss1 = ss1_int + 1
                elif 32 <= ss1_int <= 42:
                    scaled_ss1 = ss1_int + 2
                elif 43 <= ss1_int <= 50:
                    scaled_ss1 = ss1_int
            if dropdown_ss1.get() == "English Language":
                if 1 <= ss1_int <= 9:
                    scaled_ss1 = ss1_int
                elif 10 <= ss1_int <= 22:
                    scaled_ss1 = ss1_int + 1
                elif 23 <= ss1_int <= 27:
                    scaled_ss1 = ss1_int + 2
                elif 28 <= ss1_int <= 42:
                    scaled_ss1 = ss1_int + 3
                elif 43 <= ss1_int <= 46:
                    scaled_ss1 = ss1_int + 1
                elif 47 <= ss1_int <= 50:
                    scaled_ss1 = ss1_int
            scaledentry_ss1 = customtkinter.CTkEntry(study_frame, width=45, height=40, placeholder_text=scaled_ss1,
                                                     font=("Helvetica", 24, "bold"), state="normal")
            scaledentry_ss1.place(x=475, y=100)
            scaledentry_ss1.configure(state="disabled")

            # Scaling dropdown 2
            global scaled_ss2
            ss2 = entry_ss2.get()
            ss2_int = int(ss2)
            if dropdown_ss2.get() == "English":
                if 1 <= ss2_int <= 10:
                    scaled_ss2 = ss2_int - 1
                elif 10 <= ss2_int <= 16:
                    scaled_ss2 = ss2_int - 2
                elif 17 <= ss2_int <= 27:
                    scaled_ss2 = ss2_int - 3
                elif 28 <= ss2_int <= 37:
                    scaled_ss2 = ss2_int - 2
                elif 38 <= ss2_int <= 42:
                    scaled_ss2 = ss2_int - 1
                elif 43 <= ss2_int <= 50:
                    scaled_ss2 = ss2_int
            if dropdown_ss2.get() == "Literature":
                if 1 <= ss2_int <= 22:
                    scaled_ss2 = ss2_int
                elif 23 <= ss2_int <= 32:
                    scaled_ss2 = ss2_int + 1
                elif 32 <= ss2_int <= 42:
                    scaled_ss2 = ss2_int + 2
                elif 43 <= ss2_int <= 50:
                    scaled_ss2 = ss2_int
            if dropdown_ss2.get() == "English Language":
                if 1 <= ss2_int <= 9:
                    scaled_ss2 = ss2_int
                elif 10 <= ss2_int <= 22:
                    scaled_ss2 = ss2_int + 1
                elif 23 <= ss2_int <= 27:
                    scaled_ss2 = ss2_int + 2
                elif 28 <= ss2_int <= 42:
                    scaled_ss2 = ss2_int + 3
                elif 43 <= ss2_int <= 46:
                    scaled_ss2 = ss2_int + 1
                elif 47 <= ss2_int <= 50:
                    scaled_ss2 = ss2_int
            if dropdown_ss2.get() == "Japanese":
                if 1 <= ss2_int <= 8:
                    scaled_ss2 = ss2_int + 2
                elif 9 <= ss2_int <= 12:
                    scaled_ss2 = ss2_int + 3
                elif 13 <= ss2_int <= 14:
                    scaled_ss2 = ss2_int + 4
                elif 15 <= ss2_int <= 18:
                    scaled_ss2 = ss2_int + 5
                elif 19 <= ss2_int <= 22:
                    scaled_ss2 = ss2_int + 6
                elif 23 <= ss2_int <= 27:
                    scaled_ss2 = ss2_int + 7
                elif 28 <= ss2_int <= 36:
                    scaled_ss2 = ss2_int + 8
                elif 37 <= ss2_int <= 39:
                    scaled_ss2 = ss2_int + 6
                elif 40 <= ss2_int <= 44:
                    scaled_ss2 = ss2_int + 5
                elif 45 == ss2_int:
                    scaled_ss2 = ss2_int + 4
                elif 46 <= ss2_int <= 50:
                    scaled_ss2 = 50
            if dropdown_ss2.get() == "German":
                if 1 <= ss2_int <= 7:
                    scaled_ss2 = ss2_int + 2
                elif 8 <= ss2_int <= 9:
                    scaled_ss2 = ss2_int + 3
                elif 10 <= ss2_int <= 15:
                    scaled_ss2 = ss2_int + 5
                elif 16 <= ss2_int <= 18:
                    scaled_ss2 = ss2_int + 6
                elif 19 <= ss2_int <= 22:
                    scaled_ss2 = ss2_int + 7
                elif 23 <= ss2_int <= 27:
                    scaled_ss2 = ss2_int + 8
                elif 28 <= ss2_int <= 36:
                    scaled_ss2 = ss2_int + 9
                elif 37 <= ss2_int <= 41:
                    scaled_ss2 = ss2_int + 7
                elif 42 <= ss2_int <= 43:
                    scaled_ss2 = ss2_int + 6
                elif 44 == ss2_int:
                    scaled_ss2 = ss2_int + 5
                elif 45 <= ss2_int <= 50:
                    scaled_ss2 = 50
