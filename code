        def close():
            menu.deiconify()
            try:
                if feyn_window.winfo_exists():
                    feyn_window.destroy()
            except NameError:
                pass
            try:
                if SQ_window.winfo_exists():
                    SQ_window.destroy()
            except NameError:
                pass
            try:
                if space_window.winfo_exists():
                    space_window.destroy()
            except NameError:
                pass

        def feyn():
            global feyn_window
            menu.withdraw()
            feyn_window = CTkToplevel(menu)
            feyn_window.title("Feynman Technique")
            feyn_window.geometry("750x700")
            feyn_window.maxsize(750, 700)
            feyn_title = customtkinter.CTkLabel(feyn_window, text="Feynman Technique", font=("Helvetica", 40, "bold"),
                                                text_color="#246EE9")
            feyn_title.place(x=210, y=40)
            feyn_close = customtkinter.CTkButton(feyn_window, text="Close Window", command=close, width=400, height=60)
            feyn_close.place(x=175, y=600)
            feynman_overview = customtkinter.CTkLabel(feyn_window,
                                                      text="The Feynman Technique is one of the most effective methods\n used to excel "
                                                           "in studying and truly understand topics.",
                                                      font=("Helvetica", 20, "normal"))
            feynman_overview.place(x=120, y=95)
            feynman_step1title = customtkinter.CTkLabel(feyn_window, text="Step 1 - Select a concept to learn!",
                                                        font=("Helvetica", 30, "bold"))
            feynman_step1title.place(x=90, y=150)
            feynman_step1 = customtkinter.CTkLabel(feyn_window,
                                                   text="Firstly write everything you know about a topic, then \nas you learn more continue to add information to your notes.",
                                                   justify="left", font=("Helvetica", 20, "normal"))
            feynman_step1.place(x=90, y=190)
            feynman_step2title = customtkinter.CTkLabel(feyn_window, text="Step 2 - Simplify!",
                                                        font=("Helvetica", 30, "bold"))
            feynman_step2title.place(x=90, y=250)
            feynman_step2 = customtkinter.CTkLabel(feyn_window,
                                                   text="Simplify your notes into words with no jargon or complexity.\nMake it easy enough that a child would understand it.",
                                                   justify="left", font=("Helvetica", 20, "normal"))
            feynman_step2.place(x=90, y=290)
            feynman_step3title = customtkinter.CTkLabel(feyn_window, text="Step 3 - Review and Refine!",
                                                        font=("Helvetica", 30, "bold"))
            feynman_step3title.place(x=90, y=360)
            feynman_step3 = customtkinter.CTkLabel(feyn_window,
                                                   text="Write down your simplified explanation of the topic. If any of your notes -\nsound confusing, aim to review those parts of the topic and then simplify.",
                                                   justify="left", font=("Helvetica", 20, "normal"))
            feynman_step3.place(x=90, y=400)
            feynman_step4title = customtkinter.CTkLabel(feyn_window, text="Step 4 - Transmit!",
                                                        font=("Helvetica", 30, "bold"))
            feynman_step4title.place(x=90, y=460)
            feynman_step4 = customtkinter.CTkLabel(feyn_window,
                                                   text="Convey your explanation to another person.\nAnswer questions to really develop your understanding.",
                                                   justify="left", font=("Helvetica", 20, "normal"))
            feynman_step4.place(x=90, y=500)

        def SQ():
            global SQ_window
            menu.withdraw()
            SQ_window = CTkToplevel(menu)
            SQ_window.title("SQ3R Method")
            SQ_window.geometry("750x700")
            SQ_window.maxsize(750, 700)
